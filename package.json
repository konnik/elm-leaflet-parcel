{
  "name": "elm-parcel-leaflet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist ",
    "dev:admin": "parcel admin.html -d ./dist/admin -o index.html",
    "dev:web": "parcel index.html",
    "build:web": "parcel build index.html",
    "build:admin": "parcel build admin.html -d ./dist/admin -o index.html --public-url ./",
    "build": "npm run build:web && npm run build:admin",
    "deploy-deprecated": "npm run build && ./deploy.sh"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-hot": "^1.1.5",
    "node-elm-compiler": "^5.0.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-asset-copier": "^1.1.0"
  },
  "dependencies": {
    "leaflet": "^1.7.1"
  },
  "assetsPath": "images"
}